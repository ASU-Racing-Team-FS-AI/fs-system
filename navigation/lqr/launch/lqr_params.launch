<?xml version="1.0"?>
<launch>
    <!-- Physical Parameters-->
    <param name="/navigation/lqr/prepare_track/min_width"  value="3.0"/>
    <param name="/navigation/lqr/prepare_track/track_width"  value="5.0"/>
    <param name="/navigation/lqr/optimize_track/vehicle_width"  value="1.5"/>

    <!-- Track Prep Parameters-->
    <param name="/navigation/lqr/prepare_track/k_reg"   value="3"/>
    <param name="/navigation/lqr/prepare_track/s_reg"  value="5"/>
    <param name="/navigation/lqr/prepare_track/stepsize_prep"  value="1.0"/>
    <param name="/navigation/lqr/prepare_track/stepsize_reg"  value="3.0"/>

    <!-- Car Boundaries-->
    <param name="/navigation/lqr/optimize_track/safety_margin"  value="3.0"/>
    <param name="/navigation/lqr/optimize_track/curvature_boundaries"  value="0.5"/>

</launch>
