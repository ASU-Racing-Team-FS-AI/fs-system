<?xml version="1.0"?>
<launch>
	<!-- Ros topics -->
	<param name="/planning/cones_topic" value="/trapper/detected"/>
	<param name="/planning/waypoints_topic" value="/planning/waypoints"/>
	<param name="/planning/frame_id" value="velodyne_fixed"/>

	<!-- planning apf parameters -->
	<param name="/planning/kAttractive" value="1.0"/>
	<param name="/planning/kRepulsive" value="1000"/>
	<param name="/planning/repulsiveRadius" value="1.25"/>
	<param name="/planning/stepSize" value="0.2"/>
	<param name="/planning/maxIterations" value="30"/>
	<param name="/planning/goalThreshold" value="0.2"/>

	<node name="planner_node" pkg="planning_apf" type="planning_node.py"/>
	<include file="$(find waypoint_cleaner)/launch/simple_cleaner.launch"/>
</launch>
